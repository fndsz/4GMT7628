
config system
	option hostname 'Ostar'
	option timezone 'CST-8'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option zonename 'Asia/Shanghai'
	option description 'mmdvm router'
	option notes 'Power by Fndsz , BG7RIQ'
	option log_proto 'udp'
	option conloglevel '8'
	option cronloglevel '5'

config timeserver 'ntp'
	list server 'ntp.tencent.com'
	list server 'ntp1.aliyun.com'
	list server 'ntp.ntsc.ac.cn'
	list server 'cn.ntp.org.cn'

config imm_init
	option ipv6 '2'
	option lang '1'
	option system_chn '1'
	option opkg_mirror 'https://mirrors.vsean.net/openwrt'

config led
	option name '4G'
	option sysfs '4G:status'
	option trigger 'netdev'
	option dev 'eth1'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led
	option name 'TF'
	option sysfs 'TF:status'
	option trigger 'default-on'

config led
	option name 'WIFI'
	option sysfs 'Wifi:status'
	option trigger 'netdev'
	option dev 'phy0-ap0'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

